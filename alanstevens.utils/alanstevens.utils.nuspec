      <dependency id='evernote' />
      <dependency id='googlechrome' />
      <dependency id='dropbox' />
      <dependency id='curl' />
      <dependency id='wget' />
      <!-- dependency id='ruby' version="(1.8.7,1.9)" / -->
      <dependency id='ruby' version="(1.9.2,1.9.3)" />
      <dependency id='ruby.devkit' />
      <dependency id='pik' />
      <dependency id='poshgit' />
      <dependency id='gitextensions' />
      <dependency id='kickassconsole' />
      <dependency id='kickassvim' />
      <dependency id='Sysinternals' />
      <dependency id='fiddler' />
      <dependency id='filezilla' />
      <dependency id='windirstat' />
      <dependency id='lessmsi' />
      <dependency id='paint.net' />
      <dependency id='putty' />
      <dependency id='tortoisehg' />
      <dependency id='sharpkeys' />
      <dependency id='7zip.install' />
      <dependency id='ccleaner' />
      <dependency id='notepad2' />
      <dependency id='foxitreader' />
      <dependency id='p4merge' />
      <dependency id='dotpeek' />
      <dependency id='testnamingmode' />
      <dependency id='resourcesextract' />
      <dependency id='executor' />
